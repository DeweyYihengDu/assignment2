head(meta_data)
LnRR_data <- escalc(measure = "ROM",
n1i=ctrl.n, n2i = oa.n, m1i = ctrl.mean,m2i = oa.mean,sd1i = ctrl.sd,sd2i = oa.sd,
data=meta_data, var.names = c("LnRR", "v"))
